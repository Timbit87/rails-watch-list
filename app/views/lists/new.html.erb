<div class="container">
  <h1> Create a list that won't get sent to the CIA </h1>
    <div class="row justify-content-center">
        <%= simple_form_for [@list] do |f| %>
          <%= f.input :name, label: "Enter your list name:" %>
          <%= f.input :photo, as: :file, label: "Upload a photo of Maruko" %>
          <%= f.submit %>
        <% end %>
        </div>
</div>
